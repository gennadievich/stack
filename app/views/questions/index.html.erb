<h2>All questions</h2>

<% @question.each do |question| %>

    <%= render partial: 'questions/question_partial', locals: {question: question} %>

    <p>
      <%= "Tags:" if question.tags.present? %>
      <%= render partial: 'questions/tag', locals: {tags: question.tags} %>
    </p>

    <br>
<% end %>
